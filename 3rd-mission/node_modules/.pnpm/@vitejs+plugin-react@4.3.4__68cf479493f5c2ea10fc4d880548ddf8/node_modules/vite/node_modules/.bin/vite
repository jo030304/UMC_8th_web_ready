#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/82104/projects/UMC_8th_Web_레디/3rd-mission/node_modules/.pnpm/vite@6.2.4_jiti@2.4.2_lightningcss@1.29.2/node_modules/vite/bin/node_modules:/mnt/c/Users/82104/projects/UMC_8th_Web_레디/3rd-mission/node_modules/.pnpm/vite@6.2.4_jiti@2.4.2_lightningcss@1.29.2/node_modules/vite/node_modules:/mnt/c/Users/82104/projects/UMC_8th_Web_레디/3rd-mission/node_modules/.pnpm/vite@6.2.4_jiti@2.4.2_lightningcss@1.29.2/node_modules:/mnt/c/Users/82104/projects/UMC_8th_Web_레디/3rd-mission/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/82104/projects/UMC_8th_Web_레디/3rd-mission/node_modules/.pnpm/vite@6.2.4_jiti@2.4.2_lightningcss@1.29.2/node_modules/vite/bin/node_modules:/mnt/c/Users/82104/projects/UMC_8th_Web_레디/3rd-mission/node_modules/.pnpm/vite@6.2.4_jiti@2.4.2_lightningcss@1.29.2/node_modules/vite/node_modules:/mnt/c/Users/82104/projects/UMC_8th_Web_레디/3rd-mission/node_modules/.pnpm/vite@6.2.4_jiti@2.4.2_lightningcss@1.29.2/node_modules:/mnt/c/Users/82104/projects/UMC_8th_Web_레디/3rd-mission/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vite.js" "$@"
else
  exec node  "$basedir/../../bin/vite.js" "$@"
fi
